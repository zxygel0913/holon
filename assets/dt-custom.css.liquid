:root {
  /* Heading font*/

  {%- if settings.custom_font_family_1 != blank  -%}
  --DTFontTypo_Heading: {{- settings.custom_font_family_1 -}}
    {%- else -%}
    --DTFontTypo_Heading: {{ dt_sc_header_font.family }}, {{ dt_sc_header_font.fallback_families }};       
  {%- endif -%}
  --font-style-header: {{ dt_sc_header_font.style }};
  --font-weight-header: {{ dt_sc_header_font.weight }};
  --font-size-header: {{ settings.type_header_base_size }};
  --font-size-header-tablet: {{ settings.type_header_base_size_tablet }};
  --font-size-header-mobile: {{ settings.type_header_base_size_mobile }};

  /* Body font*/
  {%- if settings.custom_font_family_2 != blank  -%}
  --DTFontTypo_Body: {{- settings.custom_font_family_2 -}};
  {%- else -%}
  --DTFontTypo_Body: {{ dt_sc_base_font.family }}, {{ dt_sc_base_font.fallback_families }};
  {%- endif -%}
  --font-style-body: {{ dt_sc_base_font.style }};
  --font-weight-body: {{ dt_sc_base_font.weight }};          
  --font-weight-body--bold: {{ dt_sc_base_font_bold.weight | default: 700 }};
  --font-weight-body--bolder: {{ dt_sc_base_font_bolder.weight | default: 700 }};
  --font-size-body: {{ settings.type_base_size }}px;   
  --font-size-body-tablet: {{ settings.type_base_size_tablet }};   
  --font-size-body-mobile: {{ settings.type_base_size_mobile }};   

  /* Custom font*/

  {%- if settings.custom_font_family_3 != blank -%}
  --DTFontTypo_Custom: {{ settings.custom_font_family_3 }}
    {%- else -%}
    --DTFontTypo_Custom: {{ dt_sc_additional_font.family }}, {{ dt_sc_additional_font.fallback_families }};
  {%- endif -%}

  --font-style-additional: {{ dt_sc_additional_font.style }};
  --font-weight-additional: {{ dt_sc_additional_font.weight }};    
  --font-size-additional: {{ settings.type_additional_size }}; 
  --font-size-additional-tablet: {{ settings.type_additional_size_tablet }};   
  --font-size-additional-mobile: {{ settings.type_additional_size_mobile }}; 

  /* Other colors */
  --swiper-theme-color:  {{ settings.primary_color }} !important;
  --DTPrimaryColor: {{ settings.primary_color }};
  --DTSecondaryColor: {{ settings.secondary_color }};
  --DTTertiaryColor: {{ settings.tertiary_color }};
  --DTBodyBGColor: {{ settings.body_bg_color }};
  --DTOverlayColor: {{ settings.overlay_color | color_modify: 'alpha', 0.5 }};
  /*--DTOverlayColor: rgb(255 255 255 / 50%);*/

  --DTLinkColor: {{ settings.link_color}};
  --DTLinkHoverColor: {{ settings.link_hover_color}};
  --DTColor_Body: {{ settings.body_text_color}};
  --DTColor_Heading: {{ settings.heading_color}};
  --DTColor_Border: {{ settings.border_color}};

  /* Error and alerts */
  --DT_Error_Color: {{ settings.error_color}};
  --DT_Success_Color:{{ settings.success_color}};
  --DT_Warning_Color:{{ settings.warning_color}};
  --DT_Info_Color: {{ settings.info_color}};

  /* Blog section */
  --DT_Blog_Border_Radius: {{ settings.blog_border_radius}}px;
  --DT_Blog_Gutter_Width: {{ settings.blog_gutter_width}}px;
  --DT_Blog_BG_Color:{{ settings.blog_bg_color}};
  --DT_Blog_Link_Color:{{ settings.blog_link_color}};
  --DT_Blog_Link_Hover_Color:{{ settings.blog_link_hover_color}};
  --DT_Blog_Text_Color: {{ settings.blog_text_color}};
  --DT_Blog_Icon_Color: {{ settings.blog_icon_color}};
  --DT_Blog_Button_BG_Color: {{ settings.blog_button_bg_color}};
  --DT_Blog_Button_Text_Color:{{ settings.blog_button_text_color}};  
  --DT_Blog_Button_BG_Hover_Color: {{ settings.blog_button_bg_hover_color}};
  --DT_Blog_Button_Text_Hover_Color:{{ settings.blog_button_text_hover_color}};

  /* Overlay Style */
  --DT_Blog_Link_Color_Overlay:{{ settings.blog_link_color_overlay}};
  --DT_Blog_Link_Hover_Color_Overlay:{{ settings.blog_link_hover_color_overlay}};
  --DT_Blog_Text_Color_overlay: {{ settings.blog_text_color_overlay}};
  --DT_Blog_Icon_Color_overlay: {{ settings.blog_icon_color_overlay}};
  --DT_Blog_Button_BG_Color_overlay: {{ settings.blog_button_bg_color_overlay}};
  --DT_Blog_Button_Text_Color_overlay:{{ settings.blog_button_text_color_overlay}};  
  --DT_Blog_Button_BG_Hover_Color_overlay: {{ settings.blog_button_bg_hover_color_overlay}};
  --DT_Blog_Button_Text_Hover_Color_overlay:{{ settings.blog_button_text_hover_color_overlay}};

  /* Base Size */
  --DTFontSizeBase: {{ settings.type_base_size }}px;  

  /* Heading Size */
  --DTFontSize_H1: {{ settings.h1_size}}px;
  --DTFontSize_H2: {{ settings.h2_size}}px;
  --DTFontSize_H3: {{ settings.h3_size}}px;
  --DTFontSize_H4: {{ settings.h4_size}}px;
  --DTFontSize_H5: {{ settings.h5_size}}px;
  --DTFontSize_H6: {{ settings.h6_size}}px;

  --DT_Heading_Line_Height: {{ settings.heading_line_height}}em;
  --DT_Body_Line_Height: {{ settings.body_line_height}}em;

  --DTRadius: {{ settings.overall_border_radius }}px;
  --DTInputPadding: {{ settings.input_padding }};

  --DTInputBackground: {{ settings.input_bg }};
  --DTform_BG: {{ settings.form_bg}};	

  /* Button Style */
  --DTPadding_Btn: {{ settings.button_padding}};
  --DT_Button_BG_Color: {{ settings.button_bg_color}};
  --DT_Button_Text_Color: {{ settings.button_text_color}};  
  --DT_Button_BG_Hover_Color: {{ settings.button_bg_hover_color}};
  --DT_Button_border_Color: {{ settings.button_border_color}};
  --DT_Button_hover_border_Color: {{ settings.button_hover_border_color}};
  --DT_Button_Text_Hover_Color: {{ settings.button_text_hover_color}};
  --DT_Button_Border_Radius: {{ settings.button_border_radius}}px;
  --DT_Button_border_width: {{ settings.button_border_width}}px;

  --DTContainer: {{settings.container_width}}px;
  --DT_Lap_Container: {{settings.lap_container_width}}px;
  --DT_Tab_Container: {{settings.tab_container_width}}px;    

  --DT_Container_Spacing: {{settings.fullwidth_gap}}%;  
  --DTGutter_Width: {{settings.gutter_width}}px;

  --DTBaseTransition: {{settings.base_transitions}}; 

  --DTboxShadowcolor: {{ settings.shadow_color }};
  --DTboxShadowcolor_light: {{ settings.shadow_color_light }};

  --DTboxShadow: 0 0 10px var(--DTboxShadowcolor);
    --DTboxShadow_light: 0 0 10px var(--DTboxShadowcolor_light);

      }

