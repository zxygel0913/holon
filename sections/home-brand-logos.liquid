  .dt-sc-swiper-slider.swiper-container .dt-sc-brand-logos-section .dt-sc-brand-logo img { width: auto; }
  
  
  #shopify-section-{{section.id}}.home-brand-logos .dt-sc-swiper-slider.swiper-container .swiper-arrows .dt-sc-btn { 
