{% assign ColumnWidth = section.settings.content_width | split: '/' %}
{% assign leftColumn = ColumnWidth[0] %}
{% assign rightColumn = ColumnWidth[1] %}

{% assign listWidth = section.settings.lap_content_width | split: '/' %}
{% assign lap_leftColumn = listWidth[0] %}
{% assign lap_rightColumn = listWidth[1] %}



{% assign full = section.settings.full %}
{% assign margin = section.settings.margin | split: ',' %}
{% assign top = margin[0] %}
{% assign bottom = margin[1] %}

<div
  class="{% render 'container-settings' %}"
  style="{% render 'bg-color-image-padding' %}"
  data-section-id="{{ section.id }}"
  data-section-type="slideshow-section"
  data-autoPlay="{{ section.settings.autoplay }}"
  data-animation-speed="{{ section.settings.slideshow_speed}}">
  <div class="row {{ section.settings.custom_class_name }}">
    {% if section.settings.enable_promo_block %}
      <div class="grouped-content {% if section.settings.reverse_column %}dt-sc-reverse-columns{% endif %}">
      {% endif %}
      {% if section.settings.enable_promo_block %}
        <div class="main-block">
        {% endif %}
        <div class="dt-sc-swiper-slider swiper-container {% if section.settings.dots %} inside_element center_pagination {% endif %} {% if section.settings.arrows %}  {{ section.settings.nav_arrow_position }} {{ section.settings.center_nav_arrow_alignment }} {% endif %}">
          <div id="home-slider-{{ section.id }}">
            <div class="swiper-wrapper">
              {% for block in section.blocks %}
                {%- if block.settings.text_alignment == 'text-start' -%}
                  {%- assign style = '1' -%}
                {%- elsif block.settings.text_alignment == 'text-end' -%}
                  {%- assign style = '2' -%}
                {%- else -%}
                  {%- assign style = '3' -%}
                {%- endif -%}
                <div class="slider_style_{{style}} slider-{{ block.id }}-list swiper-slide">
                  {% include 'slider-data' %}
                </div>
              {% endfor %}
            </div>
            {% if section.settings.arrows %}
              <div class="swiper-{{ section.id}}-arrows swiper-arrows">
                <div class="swiper-button-next dt-sc-btn" id="swiper-button-next-{{section.id}}"></div>
                <div class="swiper-button-prev dt-sc-btn" id="swiper-button-prev-{{section.id}}"></div>
              </div>
            {% endif %}
            {% if section.settings.dots -%}
              <div class="swiper-pagination" id="swiper-pagination-{{section.id}}"></div>
            {% endif %}
          </div>
        </div>
        {% if section.settings.enable_promo_block %}
        </div>
      {% endif %}
      {% if section.settings.enable_promo_block %}
        <div class="additional-block dt-sc-column {{ section.settings.promo_block_style }}">

          {% if section.settings.enable_additional_block_1 %}

            <div class="dt-sc-promo-block">
              {% if section.settings.link_1 != blank %}
                <a href="{{ section.settings.link_1 }}">
                {% endif %}

                <div class="dt-sc-image-gallery">
                  {% if section.settings.image_1 != blank %}
                    <img
                      class="dt-sc-block-image lazyload"
                      src="{{ 'loading.gif' | asset_url }}"
                      data-src="{{ section.settings.image_1 | img_url: 'large' }}"
                      data-widths="[180, 360, 470, 600, 770, 970, 1060, 1280, 1512, 1728, 2048]"
                      data-aspectratio="{{ section.settings.image_1.aspect_ratio }}"
                      data-sizes="auto"
                      data-alt="{{ section.settings.title_1 }}">
                    <noscript>
                      {{ section.settings.image_1 | img_url: '480x480', scale: 2 | img_tag: section.settings.image_1.alt, 'dt-sc-noscript-image' }}
                    </noscript>
                  {% else %}
                    <img data-src="//via.placeholder.com/650x380" class="lazyload">
                  {% endif %}
                </div>
                {% if section.settings.link_1 != blank %}
                </a>
              {% endif %}
              <div class="featured-content">
                {% if section.settings.title_1 != blank %}
                  <h6>{{ section.settings.title_1 }}</h6>
                {% endif %}
                {% if section.settings.text_1 != blank %}
                  <h3>{{ section.settings.text_1 }}</h3>
                {% endif %}
                {% if section.settings.button_1 != blank %}
                  <a class="dt-sc-btn" href="{{ section.settings.link_1 }}">{{ section.settings.button_1 }}</a>
                {% endif %}
              </div>
            </div>

          {% endif %}

          {% if section.settings.enable_additional_block_2 %}

            <div class="dt-sc-promo-block">
              {% if section.settings.link_2 != blank %}
                <a href="{{ section.settings.link_2 }}">
                {% endif %}

                <div class="dt-sc-image-gallery">
                  {% if section.settings.image_2 != blank %}
                    <img
                      class="dt-sc-block-image lazyload"
                      src="{{ 'loading.gif' | asset_url }}"
                      data-src="{{ section.settings.image_2 | img_url: 'large' }}"
                      data-widths="[180, 360, 470, 600, 770, 970, 1060, 1280, 1512, 1728, 2048]"
                      data-aspectratio="{{ section.settings.image_2.aspect_ratio }}"
                      data-sizes="auto"
                      data-alt="{{ section.settings.title_2 }}">
                    <noscript>
                      {{ section.settings.image_2 | img_url: '480x480', scale: 2 | img_tag: section.settings.image_2.alt, 'dt-sc-noscript-image' }}
                    </noscript>
                  {% else %}
                    <img data-src="//via.placeholder.com/650x380" class="lazyload">
                  {% endif %}
                </div>
                {% if section.settings.link_2 != blank %}
                </a>
              {% endif %}
              <div class="featured-content">
                {% if section.settings.title_2 != blank %}
                  <h6>{{ section.settings.title_2 }}</h6>
                {% endif %}
                {% if section.settings.text_2 != blank %}
                  <h3>{{ section.settings.text_2 }}</h3>
                {% endif %}
                {% if section.settings.button_2 != blank %}
                  <a class="dt-sc-btn" href="{{ section.settings.link_2 }}">{{ section.settings.button_2 }}</a>
                {% endif %}
              </div>
            </div>

          {% endif %}

          {% if section.settings.enable_additional_block_3 %}

            <div class="dt-sc-promo-block">
              {% if section.settings.link_3 != blank %}
                <a href="{{ section.settings.link_3 }}">
                {% endif %}

                <div class="dt-sc-image-gallery">
                  {% if section.settings.image_3 != blank %}
                    <img
                      class="dt-sc-block-image lazyload"
                      src="{{ 'loading.gif' | asset_url }}"
                      data-src="{{ section.settings.image_3 | img_url: 'large' }}"
                      data-widths="[180, 360, 470, 600, 770, 970, 1060, 1280, 1512, 1728, 2048]"
                      data-aspectratio="{{ section.settings.image_3.aspect_ratio }}"
                      data-sizes="auto"
                      data-alt="{{ section.settings.title_3 }}">
                    <noscript>
                      {{ section.settings.image_3 | img_url: '480x480', scale: 2 | img_tag: section.settings.image_3.alt, 'dt-sc-noscript-image' }}
                    </noscript>
                  {% else %}
                    <img data-src="//via.placeholder.com/650x380" class="lazyload">
                  {% endif %}
                </div>
                {% if section.settings.link_3 != blank %}
                </a>
              {% endif %}
              <div class="featured-content">
                {% if section.settings.title_3 != blank %}
                  <h6>{{ section.settings.title_3 }}</h6>
                {% endif %}
                {% if section.settings.text_3 != blank %}
                  <h3>{{ section.settings.text_3 }}</h3>
                {% endif %}
                {% if section.settings.button_3 != blank %}
                  <a class="dt-sc-btn" href="{{ section.settings.link_3 }}">{{ section.settings.button_3 }}</a>
                {% endif %}
              </div>
            </div>

          {% endif %}

        </div>
      {% endif %}
      {% if section.settings.enable_promo_block %}
      </div>
    {% endif %}
  </div>
</div>



